{
  "name": "@uniformdev/cancu-stack",
  "version": "1.9.24",
  "license": "Unlicense",
  "private": true,
  "engines": {
    "pnpm": "please-use-npm",
    "yarn": "please-use-npm",
    "node": ">=18.0.0",
    "npm": ">=8.0.8"
  },
  "scripts": {
    "dev": "run-s copy:theme pull:manifest dev:next",
    "dev:next": "next dev",
    "build": "run-s copy:theme pull:manifest build:next",
    "build:next": "next build",
    "pull:manifest": "uniform context manifest download --output ./src/context/manifest.json",
    "start": "next start",
    "start-demo": "run-s pull:manifest build:next start",
    "publish:context": "uniform context manifest publish",
    "uniform:pull": "run-s uniform:pull:canvas uniform:pull:context uniform:pull:map",
    "uniform:pull:canvas": "run-s uniform:pull:components uniform:pull:compositions",
    "uniform:pull:compositions": "uniform canvas composition pull ./data/uniform/canvas/compositions",
    "uniform:pull:map:def": "uniform prm definition pull ./data/uniform/map/def",
    "uniform:pull:map:nodes": "uniform prm node pull ./data/uniform/map/nodes",
    "uniform:pull:map": "run-s uniform:pull:map:def uniform:pull:map:nodes",
    "uniform:push:map:def": "uniform prm definition push ./data/uniform/map/def",
    "uniform:push:map:nodes": "uniform prm node push ./data/uniform/map/nodes",
    "uniform:push:map": "run-s uniform:push:map:def uniform:push:map:nodes",
    "uniform:pull:components": "uniform canvas component pull ./data/uniform/canvas/components",
    "uniform:pull:context": "run-s uniform:pull:context:signals uniform:pull:context:enrichments uniform:pull:context:tests uniform:pull:context:intents",
    "uniform:pull:context:signals": "uniform context signal pull ./data/uniform/context/signals",
    "uniform:pull:context:enrichments": "uniform context enrichment pull ./data/uniform/context/enrichments",
    "uniform:pull:context:tests": "uniform context test pull ./data/uniform/context/tests",
    "uniform:pull:context:intents": "uniform context intent pull ./data/uniform/context/intents",
    "uniform:push": "run-s uniform:push:context publish:context uniform:push:canvas uniform:push:map",
    "uniform:push:context": "run-s uniform:push:context:signals uniform:push:context:enrichments uniform:push:context:tests uniform:push:context:intents",
    "uniform:push:context:signals": "uniform context signal push ./data/uniform/context/signals",
    "uniform:push:context:enrichments": "uniform context enrichment push ./data/uniform/context/enrichments",
    "uniform:push:context:tests": "uniform context test push ./data/uniform/context/tests",
    "uniform:push:context:intents": "uniform context intent push ./data/uniform/context/intents",
    "uniform:push:canvas": "run-s uniform:push:components uniform:push:compositions uniform:push:compositions:publish",
    "uniform:push:components": "uniform canvas component push ./data/uniform/canvas/components",
    "uniform:push:compositions": "uniform canvas composition push ./data/uniform/canvas/compositions",
    "uniform:push:compositions:publish": "uniform canvas composition push ./data/uniform/canvas/compositions --state published",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "lint": "eslint src",
    "generate:enrichments": "ts-node scripts/generate-enrichments.ts",
    "import:package": "ts-node scripts/import-package",
    "import:all-packages": "npm run import:package -- --articles",
    "copy:theme": "ts-node scripts/copy-theme.ts",
    "setup:contentful": "node ./scripts/setup.js"
  },
  "dependencies": {
    "@algolia/recommend": "4.14.3",
    "@commercelayer/js-auth": "^3.0.0",
    "@commercelayer/react-components": "^3.13.0",
    "@commercelayer/sdk": "^4.24.0",
    "@contentful/rich-text-html-renderer": "^15.13.1",
    "@contentful/rich-text-types": "^15.12.1",
    "@headlessui/react": "^1.7.8",
    "@heroicons/react": "^2.0.14",
    "@uniformdev/canvas": "18.34.0",
    "@uniformdev/canvas-algolia": "18.9.1-alpha.0",
    "@uniformdev/canvas-cloudinary": "17.1.1-alpha.28",
    "@uniformdev/canvas-contentful": "18.34.0",
    "@uniformdev/canvas-next": "18.34.0",
    "@uniformdev/canvas-react": "18.34.0",
    "@uniformdev/cli": "18.34.0",
    "@uniformdev/context": "18.34.0",
    "@uniformdev/context-devtools": "18.34.0",
    "@uniformdev/context-gtag": "18.34.0",
    "@uniformdev/context-next": "18.34.0",
    "@uniformdev/context-react": "18.34.0",
    "algoliasearch": "^4.15.0",
    "classnames": "^2.3.1",
    "contentful": "^9.1.34",
    "contentful-import": "^8.5.47",
    "critters": "0.0.16",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "lodash.flattendeep": "^4.4.0",
    "next": "^13.2.4",
    "nextjs-cors": "^2.1.1",
    "p-queue": "^7.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-instantsearch-hooks-web": "^6.41.0",
    "react-multi-carousel": "^2.8.2",
    "react-transition-group": "^4.4.5",
    "sass": "^1.55.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@next/eslint-plugin-next": "^12.3.1",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash": "^4.14.191",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^18.7.15",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-transition-group": "^4.4.5",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.2",
    "eslint": "^8.33.0",
    "eslint-config-next": "^13.1.6",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-import": "^15.0.0",
    "postcss-loader": "^7.0.1",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^13.0.2",
    "shx": "^0.3.4",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "yaml": "^2.1.2"
  },
  "eslintConfig": {
    "extends": [
      "custom"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint"
  }
}
